<section xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en-US" xml:id="geogebra-multiply-complex-numbers">
  <title>Multiplication of Complex Numbers</title>

  <figure xml:id="geogebra-multiply-complex-numbers-geogebra">
      <caption>Practice simplifying the product of complex numbers.</caption>
      <interactive platform="geogebra" width="100%" aspect="9:5">
        <slate surface="geogebra" aspect="9:5" source="geogebra/complex-number-multiplication.ggb">
          setPerspective("G");
          setGridVisible(0); 
          setAxesVisible(0,0);        
        </slate>
      </interactive>
    </figure>

</section>
