<section xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en-US" xml:id="radicals-additional-practice">
    <title>Additional Practice with Radicals and Rational Exponents</title>

    <exercises>

      <exercisegroup>
        <introduction>
          <p>
            Simplify each expression.  Please note that this includes factoring out perfect squares, rationalizing denominators, and combining like terms.
          </p>
        </introduction>

        <exercise>
          <statement>
            <p>
              <m>\sqrt{28}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\sqrt{28}=2\sqrt{7}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt{98}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\sqrt{98}=7\sqrt{2}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>-\sqrt{800}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>-\sqrt{800}=-20\sqrt{2}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>5\sqrt{125}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>5\sqrt{125}=25\sqrt{5}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\frac{\sqrt{32}}{8}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\frac{\sqrt{32}}{8}=\frac{\sqrt{2}}{2}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\frac{6}{\sqrt{3}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\frac{6}{\sqrt{3}}=2\sqrt{3}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\frac{4}{\sqrt{8}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\frac{4}{\sqrt{8}}=\sqrt{2}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>-\frac{2}{\sqrt{48}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>-\frac{2}{\sqrt{48}}=-\frac{\sqrt{3}}{6}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>-\frac{7}{3\sqrt{63}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>-\frac{7}{3\sqrt{63}}=-\frac{\sqrt{7}}{9}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\frac{3}{25\sqrt{200}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\frac{3}{25\sqrt{200}}=\frac{3\sqrt{2}}{500}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\frac{12}{\sqrt{288}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\frac{12}{\sqrt{288}}=\frac{\sqrt{2}}{2}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>2\sqrt{8}-\sqrt{32}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>2\sqrt{8}-\sqrt{32}=0</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt{80}+3\sqrt{20}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\sqrt{80}+3\sqrt{20}=10\sqrt{5}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>3\sqrt{18}-2\sqrt{24}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>3\sqrt{18}-2\sqrt{24}=9\sqrt{2}-4\sqrt{6}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>(2+\sqrt{7})^2</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>(2+\sqrt{7})^2=11+4\sqrt{7}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>(6-\sqrt{20})(6+\sqrt{20})</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>(6-\sqrt{20})(6+\sqrt{20})=16</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>(\sqrt{8}+3)(\sqrt{8}-3)</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>(\sqrt{8}+3)(\sqrt{8}-3)=-1</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>(1-\sqrt{72})^2</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>(1-\sqrt{72})^2=73-12\sqrt{2}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\frac{6}{\sqrt{2}}-5\sqrt{18}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\frac{6}{\sqrt{2}}-5\sqrt{18}=-12\sqrt{2}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\frac{2}{\sqrt{125}}+\sqrt{500}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\frac{2}{\sqrt{125}}+\sqrt{500}=\frac{252\sqrt{5}}{25}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\frac{30}{\sqrt{20}}+\frac{12}{\sqrt{45}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\frac{30}{\sqrt{20}}+\frac{12}{\sqrt{45}}=\frac{19\sqrt{5}}{5}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\frac{2}{\sqrt{6}-\sqrt{2}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\frac{2}{\sqrt{6}-\sqrt{2}}=\frac{\sqrt{6}+\sqrt{2}}{2}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\frac{10}{\sqrt{6}-4}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\frac{10}{\sqrt{6}-4}=-4-\sqrt{6}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\frac{\sqrt{5}+\sqrt{3}}{\sqrt{5}-\sqrt{3}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\frac{\sqrt{5}+\sqrt{3}}{\sqrt{5}-\sqrt{3}}=4+\sqrt{15}</m>
            </p>
          </solution>
        </exercise>

      </exercisegroup>

      <exercisegroup>
        <introduction>
          <p>
            Simplify each radical expression.
          </p>
        </introduction>

        <exercise>
          <statement>
            <p>
              <m>\sqrt[3]{64}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\sqrt[3]{64}=4</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>-\sqrt[4]{625}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>-\sqrt[4]{625}=-5</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>-\sqrt[5]{-243}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>-\sqrt[5]{-243}=3</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>3\sqrt[6]{64}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>3\sqrt[6]{64}=6</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>2\sqrt[3]{-343}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>2\sqrt[3]{-343}=-14</m>
            </p>
          </solution>
        </exercise>

      </exercisegroup>

      <exercisegroup>
        <introduction>
          <p>
            Convert each exponential expression to a radical expression and each radical expression to an exponential expression.   When converting to a rational exponent, reduce the exponent if possible. Assume that all variables represent positive values.
          </p>
        </introduction>

        <exercise>
          <statement>
            <p>
              <m>x^{6/17}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>x^{6/17}=\sqrt[17]{x^6}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>x^{5/2}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>x^{5/2}=\sqrt{x^5}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>y^{1/7}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>y^{1/7}=\sqrt[7]{y}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt[8]{y^5}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\sqrt[8]{y^5}=y^{5/8}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt{w^{24}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\sqrt{w^{24}}=w^{12}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt[14]{t^{21}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\sqrt[14]{t^{21}}=t^{3/2}</m>
            </p>
          </solution>
        </exercise>

      </exercisegroup>

      <exercisegroup>
        <introduction>
          <p>
            Determine the value of each expression.
          </p>
        </introduction>

        <exercise>
          <statement>
            <p>
              <m>27^{4/3}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>27^{4/3}=81</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>25^{3/2}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>25^{3/2}=125</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>16^{-3/4}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>16^{-3/4}=\frac{1}{8}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>32^{-3/5}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>32^{-3/5}=\frac{1}{8}</m>
            </p>
          </solution>
        </exercise>

      </exercisegroup>

      <exercisegroup>
        <introduction>
          <p>
            Simplify each radical expression after first rewriting the expression in exponential form.  Assume that all variables represent positive values.
          </p>
        </introduction>

        <exercise>
          <statement>
            <p>
              <m>\sqrt[4]{x^{10}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\sqrt[4]{x^{10}}=\sqrt{x^5}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt{x}\sqrt[6]{x}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\sqrt{x}\sqrt[6]{x}=\sqrt[3]{x^2}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt[3]{x^6y^7}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\sqrt[3]{x^6y^7}=x^2y^2\sqrt[3]{y}</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt{\sqrt[4]{y^{24}}}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>\sqrt{\sqrt[4]{y^{24}}}=y^3</m>
            </p>
          </solution>
        </exercise>

      </exercisegroup>
      <exercisegroup>
        <introduction>
          <p>
            Determine the solution set to each equation.
          </p>
        </introduction>

        <exercise>
          <statement>
            <p>
              <m>\sqrt{2x+1}=3</m>
            </p>
          </statement>
          <solution>
            <p>
              The solution set is <m>\{4\}</m>.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>2\sqrt[3]{1-\frac{t}{8}}=3</m>
            </p>
          </statement>
          <solution>
            <p>
              The solution set is <m>\{-19\}</m>.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt{3-x}=-4</m>
            </p>
          </statement>
          <solution>
            <p>
              The solution set is <m>\emptyset</m>.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt[5]{6w+4}=2</m>
            </p>
          </statement>
          <solution>
            <p>
              The solution set is <m>\{\frac{14}{3}\}</m>.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>3\sqrt{y-4}+5=14</m>
            </p>
          </statement>
          <solution>
            <p>
              The solution set is <m>\{13\}</m>.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt[3]{2x+7}+11=5-\sqrt[3]{2x+7}</m>
            </p>
          </statement>
          <solution>
            <p>
              The solution set is <m>\{-17\}</m>.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>-\frac{\sqrt{8-t}}{3}+6=1</m>
            </p>
          </statement>
          <solution>
            <p>
              The solution set is <m>\{-217\}</m>.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>x=3+\sqrt{x-1}</m>
            </p>
          </statement>
          <solution>
            <p>
              The solution set is <m>\{5\}</m>.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt{7-x}-4\sqrt{x+10}=0</m>
            </p>
          </statement>
          <solution>
            <p>
              The solution set is <m>\{-9\}</m>.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt{3x+4}=2-\sqrt{x+2}</m>
            </p>
          </statement>
          <solution>
            <p>
              The solution set is <m>\{-1\}</m>.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt{2y-5}-3\sqrt{y+1}=-7</m>
            </p>
          </statement>
          <solution>
            <p>
              The solution set is <m>\{15\}</m>.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>\sqrt{6t+7}-\sqrt{3t+3}=1</m>
            </p>
          </statement>
          <solution>
            <p>
              The solution set is <m>\{-1,\frac{1}{3}\}</m>.
            </p>
          </solution>
        </exercise>

      </exercisegroup>

    </exercises>

  </section>