                              xtick={-0.25,-0.125,0.125,0.25,0.375,0.5,0.6125,0.75},
                              ytick={-1,1},
                              minor ytick={-0.5,0.5},
                              xticklabels={$-\frac{1}{4}$,$-\frac{1}{8}$,$\frac{1}{8}$,$\frac{1}{4}$,$\frac{3}{8}$,$\frac{1}{2}$,$\frac{5}{8}$,$\frac{3}{4}$}]
                  \addplot [domain=-0.25:0.75,samples=100,color=blue,line width=1.5pt] {-sin(720*x-180)};                        
                \end{axis}
