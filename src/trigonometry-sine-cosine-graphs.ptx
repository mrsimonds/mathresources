              \addplot [primarycurve,domain=-0.25:0,line width=1pt,-,color=purple!75!black] {1.733*x} node[pos=0,below left] {$\frac{4\pi}{3}$};
              \addplot [primarycurve,domain=-0.5:-0.36,line width=1pt,-,color=purple!75!black] {1.733*x};    
              \addplot [primarycurve,domain=-0.41:0,line width=1pt,-,color=brown] {.577*x} node[pos=0,below left] {$\frac{7\pi}{6}$};  
              \addplot [primarycurve,domain=-0.866:-0.54,line width=1pt,-,color=brown] {.577*x}; 
              \addplot [interval,yellow!30,-,line width=1pt,-] coordinates {(0,-0.7) (0,-0.55)};
              \addplot [interval,blue,-,line width=1pt,-] coordinates {(0,-0.55) (0,0)} node[pos=0,below] {$\frac{3\pi}{2}$};
              \addplot [interval,blue,-,line width=1pt,-] coordinates {(0,-1) (0,-0.7)};
              \addplot [soliddot,color=blue] coordinates {(0,-1)} node[below right] {$(0,-1)$};
              \addplot [interval,yellow!30,-,line width=1pt,-] coordinates {(-0.66,0) (-0.55,0)};
              \addplot [interval,blue!50!purple,-,line width=1pt,-] coordinates {(-0.55,0) (0,0)} node[pos=0,left] {$\pi$};
              \addplot [interval,blue!50!purple,-,line width=1pt,-] coordinates {(-1,0) (-0.66,0)};
