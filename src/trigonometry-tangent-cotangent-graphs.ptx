              \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(-360,-6.48) (-360,6.48)};
              \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(-180,-6.48) (-180,6.48)}; 
              \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(180,-6.48) (180,6.48)};
              \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(360,-6.48) (360,6.48)};
              \addplot [soliddot,color=blue] coordinates {(-225,-1)};
              \addplot [soliddot,color=blue] coordinates {(-135,1)};
              \addplot [soliddot,color=blue] coordinates {(-90,0)};
              \addplot [soliddot,color=blue] coordinates {(-45,-1)};
