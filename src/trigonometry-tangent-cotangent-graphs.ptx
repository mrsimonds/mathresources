                              minor ytick={},
                              xticklabels={$-\frac{7\pi}{6}$,$-\frac{2\pi}{3}$,$-\frac{\pi}{6}$,$\frac{\pi}{3}$,$\frac{5\pi}{6}$,$\frac{4\pi}{3}$,$\frac{11\pi}{6}$}]
                  \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(-210,-6.48) (-210,6.48)}; 
                  \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(-30,-6.48) (-30,6.48)};
                  \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(330,-6.48) (330,6.48)}; 
                  \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(150,-6.48) (150,6.48)};
                \end{axis}
              \end{tikzpicture}
              ]]>
