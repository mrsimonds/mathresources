<section xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en-US" xml:id="test-page-geogebra">
  <title>Geogebra Applet Test Page</title>
  
  
  <figure xml:id="factoring-geogebra-1">
    <caption>Test Object</caption>
    <interactive platform="geogebra" width="100%" aspect="9:5">
      <slate surface="geogebra" aspect="9:5" source="geogebra/exponents-positive.ggb">
        setPerspective("G");
        setGridVisible(0); 
