              \addplot [domain=-480:480,samples=200,color=gray,line width=.75pt,{Stealth}-{Stealth}] {sin(x)};
              \addplot [domain=-480:-369,samples=200,color=red,line width=1.5pt,{Stealth}-{Stealth}] {1/sin(x)};
              \addplot [domain=-351:-189,samples=200,color=red,line width=1.5pt,{Stealth}-{Stealth}] {1/sin(x)};
              \addplot [domain=-171:-9,samples=200,color=red,line width=1.5pt,{Stealth}-{Stealth}] {1/sin(x)};
