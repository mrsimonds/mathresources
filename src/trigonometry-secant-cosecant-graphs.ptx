                      \addplot [domain=-1.95:-1.05,samples=200,color=red,line width=1.5pt,{Stealth}-{Stealth}] {1/sin(180*x+180)};
                      \addplot [domain=-0.95:-0.05,samples=200,color=red,line width=1.5pt,{Stealth}-{Stealth}] {1/sin(180*x+180)};
                      \addplot [domain=0.05:0.95,samples=200,color=red,line width=1.5pt,{Stealth}-{Stealth}] {1/sin(180*x+180)};
                      \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(-3,-6.48) (-3,6.48)};
                      \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(-2,-6.48) (-2,6.48)};
                      \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(-1,-6.48) (-1,6.48)};
                      \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(1,-6.48) (1,6.48)};
                      \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(2,-6.48) (2,6.48)};
                      \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(3,-6.48) (3,6.48)};
                    \end{axis}
                  \end{tikzpicture}
                ]]>
