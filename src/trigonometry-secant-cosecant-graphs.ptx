                    \addplot [domain=-675:765,samples=200,color=gray,line width=.75pt,-] {-2*sin(x/2-22.5)};
                    \addplot [domain=-638:-352,samples=200,color=red,line width=1.5pt,{Stealth}-{Stealth}] {-2/sin(x/2-22.5)};
                    \addplot [domain=-278:8,samples=200,color=red,line width=1.5pt,{Stealth}-{Stealth}] {-2/sin(x/2-22.5)};
                    \addplot [domain=82:368,samples=200,color=red,line width=1.5pt,{Stealth}-{Stealth}] {-2/sin(x/2-22.5)};
                    \addplot [domain=442:728,samples=200,color=red,line width=1.5pt,{Stealth}-{Stealth}] {-2/sin(x/2-22.5)};
                    \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(-675,-6.48) (-675,6.48)};
                    \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(-315,-6.48) (-315,6.48)};
                    \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(45,-6.48) (45,6.48)};
                    \addplot[interval,thin,black,style=dashed,{Stealth}-{Stealth}] coordinates {(405,-6.48) (405,6.48)};
