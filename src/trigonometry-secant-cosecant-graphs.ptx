              \addplot [domain=-480:-459,samples=200,color=red,line width=1.5pt,{Stealth}-{Stealth}] {sec(x)};
              \addplot [domain=-441:-279,samples=200,color=red,line width=1.5pt,{Stealth}-{Stealth}] {sec(x)};
