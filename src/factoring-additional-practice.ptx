<section xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en-US" xml:id="factoring-addtional-practice">
    <title>Additional Practice Factoring</title>

    <exercises>

      <exercisegroup>
        <introduction>
          <p>Completely factor each expression.</p>
        </introduction>

        <exercise>
          <statement>
            <p>
              <m>4x^2-10xy+16x^2y^2</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>4x^2-10xy+16x^2y^2=2x(2x-5y+8xy^2)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>-12a^2b^3c^4+18ab^3c^5-60ab^2c^4</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>-12a^2b^3c^4+18ab^3c^5-60ab^2c^4=-6ab^2c^4(2ab-3bc+10)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>45x^7y^{11}+75x^6y^{12}+15x^6y^9</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>45x^7y^{11}-75x^6y^{12}+15x^6y^9=15x^6y^9(3xy^2-5y^3+1)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>x^2-10x-24</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>x^2-10x-24=(x-12)(x+2)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>x^2+11x+30</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>x^2+11x+30=(x+6)(x+5)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>y^2-12y+36</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>y^2-12y+36=(y-6)(y-6)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>w^2+12w+24</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>w^2+12w+24</m> is prime.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>w^2+21w+80</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>w^2+21w+80=(w+15)(w+6)</m>
            </p>
          </solution>
        </exercise> 

        <exercise>
          <statement>
            <p>
              <m>x^2y+4x-3xy^2-12y</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>x^2y+4x-3xy^2-12y=(xy+4)(x-3y)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>12x^2+8x-21x-14</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>12x^2+8x-21x-14=(3x+2)(4x-7)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>4a^2b-8ab^2+a-2b</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>4a^2b-8ab^2+a-2b=(a-2b)(4ab+1)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>4x^2-17x-15</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>4x^2-17x-15=(4x+3)(x-5)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>6x^2+17x+5</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>6x^2+17x+5=(2x+5)(3x+1)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>2x^2+5x-4</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>2x^2+5x-4</m> is prime.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>3x^2-10x-8</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>3x^2-10x-8=(x-4)(3x+2)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>16x^2-8x+1</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>16x^2-8x+1=(4x-1)(4x-1)</m>
            </p>
          </solution>
        </exercise>
                
        <exercise>
          <statement>
            <p>
              <m>x^3+8</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>x^3+8=(x+2)(x^2-2x+4)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>w^2-64</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>w^2-64=(w-8)(w+8)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>y^2+49</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>y^2+49</m> is prime.
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>1-27x^3</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>1-27x^3=(1-3x)(1+3x+9x^2)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>100-9y^{10}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>100-9y^{10}=(10-3y^5)(10+3y^5)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>w^2-3w-88</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>w^2-3w-88=(w-11)(w+8)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>3x^2-54x+240</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>3x^2-54x+240=3(x-10)(x-2)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>3x^2-11x-20</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>3x^2-11x-20=(3x+4)(x-5)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>3x^3-81y^{15}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>3x^3-81y^{15}=3(x-3y^5)(x^2+3xy^5+9y^{10})</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>4x^2-16y^{12}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>4x^2-16y^{12}=4(x-2y^6)(x+2y^6)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>4x^2+16y^{12}</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>4x^2+16y^{12}=4(x^2+4y^{12})</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>20x^2-3xy-2y^2</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>20x^2-3xy-2y^2=(4x+y)(5x-2y)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>1+125w^9</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>1+125w^9=(1+5w^3)(1-5w^3+25w^6)</m>
            </p>
          </solution>
        </exercise>

        <exercise>
          <statement>
            <p>
              <m>xy^4+6x^2y^3-40x^3y^2</m>
            </p>
          </statement>
          <solution>
            <p>
              <m>xy^4+6x^2y^3-40x^3y^2=xy^2(y-4x)(y+10x)</m>
            </p>
          </solution>
        </exercise>

      </exercisegroup>

    </exercises>

  </section>

              