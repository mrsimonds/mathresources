<section xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en-US" xml:id="exponents-additional-practice">
  <title>Additional Practice with Exponents and Scientific Notation</title>

  <exercises>

    <exercisegroup cols="2">
      <introduction>
        <p>
          Completely simplify each expression.  Your final expression should contain no negative exponents.  Assume that all variables represent positive numbers.
        </p>
      </introduction>

      <exercise>
        <statement>
          <p>
            <m>
              x^8 \cdot x^{21}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              x^8 \cdot x^{21}=x^{29}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              (x^7)^{10}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              (x^7)^{10}=x^{70}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
           <p>
            <m>
              \frac{x^{17}}{x^{11}}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \frac{x^{17}}{x^{11}}=x^6
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              \frac{x^{14}}{x^{23}}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \frac{x^{14}}{x^{23}}=\frac{1}{x^9}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              2x^{-31}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              2x^{-31}=\frac{2}{x^{31}}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              \frac{1}{x^{-96}}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \frac{1}{x^{-96}}=x^{96}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              -x^{-2}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              -x^{-2}=-\frac{1}{x^2}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              (x^4y^{-3})^5
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              (x^4y^{-3})^5=\frac{x^{20}}{y^{15}}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              \frac{x^4x^{-6}}{5x^{-2}}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \frac{x^4x^{-6}}{5x^{-2}}=\frac{1}{5}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              \left(\frac{2x^{-7}}{y^{-2}}\right)^{-6}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \left(\frac{2x^{-7}}{y^{-2}}\right)^{-6}=\frac{x^{42}}{64y^{12}}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              \left(\frac{4x^{-1}y^{12}z^{-3}}{(x^2y^{-3})^{-6}}\right)^0
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \left(\frac{4x^-1y^{12}z^{-3}}{(x^2y^{-3})^{-6}}\right)^0=1
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              \frac{(xy^{-4})^{-2}}{(3x^{-1})^2}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \frac{(xy^{-4})^{-2}}{(3x^{-1})^2}=\frac{y^8}{9}
            </m>
          </p>
        </solution>
      </exercise>

    </exercisegroup>
    <exercisegroup>
      <introduction>
        <p>
          Write each number in scientific notation.
        </p>
      </introduction>

      <exercise>
        <statement>
          <p>
            <m>
              321,000,000
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              321,000,000=3.21\times 10^{8}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              -0.00067
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              -0.00067=-6.7\times 10^{-4}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              -11,248,760,000
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              -11,248,760,000=-1.124876\times 10^{10}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              0.0000000600
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              0.0000000600=6.00\times 10^{-8}
            </m>
          </p>
        </solution>
      </exercise>


    </exercisegroup>
    <exercisegroup>
      <introduction>
        <p>
          Write each number in standard notation.
        </p>
      </introduction>

      <exercise>
        <statement>
          <p>
            <m>
              3.2\times 10^{-7}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              3.2\times 10^{-7}=0.00000032
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              2.7\times 10^{5}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              2.7\times 10^{5}=270,000
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              -1.190\times 10^{8}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>-1.190\times 10^{8}=-119,000,000</m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              -9.99\times 10^{-9}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              -9.99\times 10^{-9}=-0.00000000999
            </m>
          </p>
        </solution>
      </exercise>


    </exercisegroup>
    <exercisegroup>
      <introduction>
        <p>
          Determine each product or quotient<mdash/>write the results using both scientific notation and standard notation.
        </p>
      </introduction>

      <exercise>
        <statement>
          <p>
            <m>
              (4.0\times 10^{-7})(-1.5\times 10^{9})
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \begin{aligned}[t]
              (4.0\times 10^{-7})(-1.5\times 10^{9})\amp=-6.0\times 10^2\\
              \amp=-600
              \end{aligned}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              (1.2\times 10^{-3})(9.0\times 10^{-4})
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \begin{aligned}[t]
              (1.20\times 10^{-3})(9.00\times 10^{-4})\amp=1.08\times 10^{-6}\\
              \amp=0.00000108
              \end{aligned}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              (-2.5\times 10^{13})(-5.0\times 10^{-7})
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \begin{aligned}[t]
              (-2.50\times 10^{13})(-5.00\times 10^{-7})\amp=1.25\times 10^{7}\\
              \amp=12,500,000
              \end{aligned}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              \frac{4.2\times 10^{11}}{6.0\times 10^{7}}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \begin{aligned}[t]
              \frac{4.20\times 10^{11}}{6.00\times 10^{7}}\amp=7.00\times 10^{3}\\
              \amp=7,000
              \end{aligned}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              \frac{-3.3\times 10^{-6}}{-3.0\times 10^{-3}}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \begin{aligned}[t]
              \frac{-3.3\times 10^{-6}}{-3.0\times 10^{-3}}\amp=1.1\times 10^{-3}\\
              \amp=0.0099
              \end{aligned}
            </m>
          </p>
        </solution>
      </exercise>

      <exercise>
        <statement>
          <p>
            <m>
              \frac{2.00\times 10^{2}}{6.00\times 10^{-1}}
            </m>
          </p>
        </statement>
        <solution>
          <p>
            <m>
              \begin{aligned}[t]
              \frac{2.00\times 10^{2}}{6.00\times 10^{-1}}\amp=3.33\times 10^{2}\\
              \amp=333
              \end{aligned}
            </m>
          </p>
        </solution>
      </exercise>

    </exercisegroup>

  </exercises>

</section>
