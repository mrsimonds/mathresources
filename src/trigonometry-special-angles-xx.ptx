                            width=8cm,
                            xmin=-1.25,xmax=1.25,
                            ymin=-1.25,ymax=1.25,
                            xtick={-1,1},
                            ytick={-1,1}]
                \addplot [domain=0:360,samples=100,color=gray,line width=1pt] ({cos(x)},{sin(x)});
                \addplot [domain=-0.799:0,color=blue,line width=1.5pt] {0.754*x}; 
                \addplot [domain=180:217,samples=100,color=black,line width=1pt,-] ({0.2*cos(x)},{0.2*sin(x)}) node[pos=0.8,left] {$\theta^{\,\prime}$}; 
