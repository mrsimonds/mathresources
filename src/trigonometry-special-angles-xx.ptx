          <md>
            <mrow>\cot{\left(135^{\circ}\right)}\amp=\frac{x}{y}</mrow>
            <mrow>\amp=\frac{-\frac{\sqrt{2}}{2}}{\frac{\sqrt{2}}{2}}</mrow>
