        <solution>
          
          <p>
            Problem 1
          </p>
          <sidebyside widths="50% 50%">
          <p>
            The domain is <m>[-4,5),</m>.  The range is <m>[-1,5]</m>. 
            
          </p>
          <p>       
          <figure xml:id="workshop-mth-65-pt-1-sineish-graph-3-key">
                    <caption><m>y=f(x)</m></caption>
                      <image>
                        <description>plain text</description>
                          <latex-image>
                            <![CDATA[
                            \begin{tikzpicture}
                              \begin{axis}
                                \addplot[primarycurve,domain=-4:5,smooth,-] {2+3*sin(pi*deg(x)/2)};
                                \addplot[soliddot] coordinates {(-4,2)};
                                \addplot[hollowdot] coordinates {(5,5)};
                                \addplot[primarycurve,domain=-4:5,blue,ultra thick,[-)] {0};
                                \addplot[closedinterval,gray,ultra thick] coordinates {(0,-1) (0,5)};
                              \end{axis}
                            \end{tikzpicture}
                            ]]>
                          </latex-image>
                      </image>
          </figure>
        </p>
        </sidebyside>
          <p>
            <bold>Problem 2</bold>
          </p>
          <sidebyside widths="50% 50%">
          <p>
            The domain is <m>(-6,\infty)</m>.  The range is <m>(-\infty,6]</m>. 
            
          </p>
          <p>
          
          <figure xml:id="workshop-mth-65-pt-1-piecewise-graph-1-3-key">
                    <caption><m>y=g(x)</m></caption>
                      <image>
                        <description>plain text</description>
                          <latex-image>
                            <![CDATA[
                            \begin{tikzpicture}
                              \begin{axis}
                                \addplot[primarycurve,domain=-6:-1,smooth,-] {(x+4)^2-6};
                                \addplot[primarycurve,domain=-1:5.26,smooth,-{Stealth}] {-2*x+4};
                                \addplot[hollowdot] coordinates {(-6,-2)};
                                \addplot[hollowdot] coordinates {(-1,3)};
                                \addplot[soliddot] coordinates {(-1,6)}; 
                                \addplot[primarycurve,domain=-6:6.5,blue,ultra thick,(-{Stealth}] {0};
                                \addplot[interval,gray,ultra thick,{Stealth}-{Bracket}] coordinates {(0,-6.5) (0,6)};
                              \end{axis}
                            \end{tikzpicture}
                            ]]>
                          </latex-image>
                      </image>
          </figure>
        </p>
      </sidebyside>

          <p>
            <bold>Problem 3</bold>
          </p>
          <sidebyside widths="50% 50%">
